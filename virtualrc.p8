__lua__
-- wander demo
-- by zep

x=24 y=24 -- position (in tiles)
dx=0 dy=0 -- velocity
f=0       -- frame number
d=1       -- direction (-1, 1)
LOCAL_DATA_SIZE = 6
NETWORK_DATA_PLAYER_SIZE = 4


function _draw()
	cls(1)
	
	-- move camera to current room
	room_x = flr(x/16)
	room_y = flr(y/16)
	camera(room_x*128,room_y*128)
	
	-- draw the whole map (128⁙32)
	map()
	
	-- draw the player
	spr(1+f,      -- frame index
	 x*8-4,y*8-4, -- x,y (pixels)
	 1,1,d==-1    -- w,h, flip
	)

	_write_local_data(room_x, room_y)
	_process_network_data()
end

function _update()
	
	ac=0.1 -- acceleration
	
	if (btn(⬅️)) dx-= ac d=-1
	if (btn(➡️)) dx+= ac d= 1
	if (btn(⬆️)) dy-= ac
	if (btn(⬇️)) dy+= ac
	
	-- move (add velocity)
	x+=dx y+=dy
	
	-- friction (lower for more)
	dx *=.7
	dy *=.7
	
	-- advance animation according
	-- to speed (or reset when
	-- standing almost still)
	spd=sqrt(dx*dx+dy*dy)
	f= (f+spd*2) % 4 -- 4 frames
	if (spd < 0.05) f=0
end

-->8
-- network

function _gpio_index_to_memory_address(index)
	return 24448 + index
end

function _write_byte_to_index(index, byte)
	return poke(_gpio_index_to_memory_address(index), byte)
end

function _read_memory_at_index(index)
	return @_gpio_index_to_memory_address(index)
end

function _write_local_data(room_x, room_y)
	for index, value in ipairs({room_x, room_y, x, y, d, 1+f}) do
		_write_byte_to_index(index - 1, value)
	end
end

function _read_network_data()
	_read_memory_at_index(LOCAL_DATA_SIZE - 1)
end

function _process_network_data()
	network_data = _read_network_data()

end

-->8
-- helpers
function enum(names, offset)
	offset=offset or 1
	local objects = {}
	local size=0
	for idr,name in pairs(names) do
		local id = idr + offset - 1
		local obj = {
			id=id,       -- id
			idr=idr,     -- 1-based relative id, without offset being added
			name=name    -- name of the object
		}
		objects[name] = obj
		objects[id] = obj
		size=size+1
	end
	objects.idstart = offset        -- start of the id range being used
	objects.idend = offset+size-1   -- end of the id range being used
	objects.size=size
	objects.all = function()
		local list = {}
		for _,name in pairs(names) do
			add(list,objects[name])
		end
		local i=0
		return function() i=i+1 if i<=#list then return list[i] end end
	end
	return objects
end

